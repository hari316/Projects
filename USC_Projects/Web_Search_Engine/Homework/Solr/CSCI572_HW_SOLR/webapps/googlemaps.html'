<!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no" />
    <style type="text/css">
      html { height: 100% }
      body { height: 100%; margin: 0; padding: 0 }
      #map-canvas { height: 100% }
    </style>
    <script type="text/javascript"
      src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBheAQq1Z6dqTD1s5MnMj5uzSx-AwXBWe8&sensor=true">
    </script>
    <script type="text/javascript">
        var markers = [],map;
      function initialize() {
        var mapOptions = {
     		zoom: 5,
            center: new google.maps.LatLng(32.7150,-117.1625),
            mapTypeId: google.maps.MapTypeId.ROADMAP
        };
        map = new google.maps.Map(document.getElementById("map-canvas"),
            mapOptions);
         addMarker(new google.maps.LatLng(32.7150,-117.1625), "Alien");
	 addMarker(new google.maps.LatLng(34.7150,-114.1625), "BB");
	
      }
      google.maps.event.addDomListener(window, 'load', initialize);
   	function addMarker(latlng, myTitle) {
          markers.push(new google.maps.Marker({
            position: latlng, 
            map: map,
            title: myTitle,
            icon: "http://maps.google.com/mapfiles/marker" + String.fromCharCode(markers.length + 65) + ".png"
          }));    
        }
    </script>
  </head>
  <body>
    <div id="map-canvas"/>
